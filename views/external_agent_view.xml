<?xml version="1.0"?>
<odoo>
	<record id="external_agents_action" model="ir.actions.act_window">
    	<field name="name">External Agent</field>
    	<field name="res_model">external.agent</field>
    	<field name="view_mode">tree,form</field>
	</record>

	<record id="external_agents_view_tree" model="ir.ui.view">
        <field name="name">external.agent.tree</field>
        <field name="model">external.agent</field>
        <field name="arch" type="xml">
            <tree string="Channel">
                <field name="name"/>
                <field name="age"/>
                <field name="min_sales"/>
                <field name="apply_rappel"/>
                <field name="salary"/>
            </tree>
        </field>
    </record>
</odoo>